<html lang="en" style="height: auto; min-height: 100%;">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title>Ovio -  Bootstrap Based Responsive Dashboard &amp; Admin Template</title>

    <!-- Bootstrap -->
    <link rel="stylesheet" href="~/lib/bootstrap4/css/bootstrap.min.css">

    <!-- Google Font -->
    <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700" rel="stylesheet">

    <!-- Template style -->
    <link rel="stylesheet" href="~/lib/dist/css/style.css">
    <link rel="stylesheet" href="~/lib/pages/et-line-font/et-line-font.css">
    <link rel="stylesheet" href="~/lib/pages/font-awesome/css/font-awesome.min.css">
    <link type="text/css" rel="stylesheet" href="~/lib/dist/weather/weather-icons.min.css">
    <link type="text/css" rel="stylesheet" href="~/lib/dist/weather/weather-icons-wind.min.css">

</head>

<body class="body-bg-color" style="height: auto; min-height: 100%;">
    <div class="wrapper" style="height: auto; min-height: 100%;">
        <div class="form-body">
            <div class="col-form">
                <div class="col-logo"><a href="#"><img alt="" src="~/lib/dist/img/logo-lg.png"></a></div>
                @RenderBody()
            </div>
        </div>
    </div>
    <script src="~/lib/dist/js/jquery.min.js"></script>
    <script src="~/lib/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/lib/dist/js/ovio.js"></script>

</body>
</html>